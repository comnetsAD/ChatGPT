#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, j, len;
    printf("Enter a string: ");
    scanf("%s", str);
    len = strlen(str);
    for(i = len-1; i >= 0; i--) {
        for(j = i; j < len; j++) {
            printf("%c", str[j]);
        }
        printf("\n");
    }
    return 0;
}

