

# create a list of numbers from 0 to 100
numbers = list(range(101))

# use a for loop to iterate over the numbers
for number in numbers:
    # create an empty output string
    output = ""
    # if the number is divisible by 3, add "FIZZ" to the output string
    if number % 3 == 0:
        output += "FIZZ"
    # if the number is divisible by 5, add "BUZZ" to the output string
    if number % 5 == 0:
        output += "BUZZ"
    # if the output string is empty, set it to the number itself
    if output == "":
        output = str(number)
    # print the output string
    print(output)


